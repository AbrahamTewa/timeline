{
   "coverageDirectory": "./reports/tests/unit/coverage/",
   "name": "starter.react",
   "roots": [
        "./src"
   ],
   "setupFiles": [
        "./enzyme.setup.js"
   ],
   "testMatch": [
        "**/*.spec.js?(x)"
   ],
   "verbose": true,

   "reporters": [
       "default",
       [
           "jest-junit",
           {
                "outputDirectory": "./reports/tests/unit",
                "outputName": "xunit.xml"
           }
       ]
   ]
}
