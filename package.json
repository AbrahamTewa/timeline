    "jsdoc": "^3.4.3",
    "jsdoc-oblivion": "^0.0.5",
    "load-grunt-tasks": "^3.5.2",
    "minami": "^1.1.1",
    "mocha": "^3.2.0",
    "plato": "^1.7.0",
    "react-hot-loader": "next",
