    "react-test-renderer": "15",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.5",
    "sc5-styleguide": "^1.9.0",
    "sinon": "^4.0.1",
    "source-map-support": "^0.4.6",
    "style-loader": "^0.18.2",
    "stylelint-webpack-plugin": "^0.7.0",
    "uglify-es": "^3.0.15",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
