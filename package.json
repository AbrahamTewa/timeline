{
  "name": "Timeline",
  "version": "0.1.0",
  "description": "Timeline standand alone page",
  "author": "Abraham Tewa",
  "license": "MIT",
  "homepage": "https://github.com/AbrahamTewa/timeline",
  "engines": {
    "node": ">=6.11.0"
  },
  "private": true,
  "scripts": {
    "analysis": "grunt clean:analysis && plato --dir reports/analysis --recurse --title \"Starter React\" src/**/*.js",
    "babel": "babel src --out-dir dist",
    "babel:watch": "babel src --out-dir dist --watch",
    "build": "cross-env NODE_ENV=production grunt pre-build && webpack -p",
    "clean": "grunt clean",
    "dev": "webpack-dev-server",
    "doc": "npm run doc:jsdoc && npm run doc:storybook",
    "doc:jsdoc": "grunt clean:jsdoc && jsdoc -c ./jsdoc.config.json --readme README.md",
    "doc:storybook": "grunt clean:storybook && build-storybook -c storybook -o doc/storybook",
    "lint": "grunt lint --force",
    "start": "node server.js ",
    "test": "jest --config jest.config.json",
    "test-u": "jest --config jest.config.json --updateSnapshot",
    "test-cov": "grunt clean:coverage && jest --config jest.config.json --coverage",
    "watch": "webpack-dev-server --progress --inline --hot",
    "watch:storybook": "start-storybook -p 9001 -c storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbrahamTewa/timeline"
  },
  "dependencies": {
    "es6-error": "^4.0.2",
    "event-target-shim": "^2.0.0",
    "express": "^4.15.3",
    "faker": "^4.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "uuid": "^3.0.1",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.1.8",
    "@storybook/addon-storyshots": "^3.1.8",
    "@storybook/react": "^3.1.8",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.10.4",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "eslint": "^3.11.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.2.0",
    "grunt-eslint": "^19.0.0",
    "grunt-gulp": "^1.0.2",
    "grunt-jsonlint": "^1.1.0",
    "grunt-sass": "^2.0.0",
    "grunt-stylelint": "^0.8.0",
    "grunt-webpack": "^3.0.0",
    "gulp": "^3.9.1",
    "istanbul": "^1.1.0-alpha.1",
    "jest": "^20.0.4",
    "js-yaml": "^3.8.4",
    "jsdoc": "^3.4.3",
    "jsdoc-oblivion": "^0.0.5",
    "load-grunt-tasks": "^3.5.2",
