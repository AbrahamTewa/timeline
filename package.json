    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "remove": "^0.1.5",
    "uuid": "^3.0.1",
    "yargs": "^10.0.3"
  },
