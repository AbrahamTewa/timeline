  "license": "MIT",
  "homepage": "https://github.com/AbrahamTewa/timeline",
  "private": true,
  "scripts": {
    "analysis": "plato --dir analysis --recurse --eslint .eslintrc.json --title \"Starter server\" src/**/*.js",
    "build": "webpack -p",
    "build-page": "git checkout master && grunt build && git checkout gh-pages && cp build/ .",
    "debugger": "devtool build/index.js",
    "dev": "webpack-dev-server",
    "doc": "grunt clean:doc && jsdoc -c jsdoc.json --readme README.md",
    "lint": "grunt lint",
    "start": "node build/index.js ",
    "test": "mocha src\\**\\__test__\\**\\*.js --opts mocha.opts -R spec",
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- src\\**\\__test__\\**\\*.js --opts mocha.opts -R spec",
    "watch": "webpack --progress --watch",
    "watch:stylesheets": "grunt watch:stylesheets"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbrahamTewa/timeline"
  },
  "dependencies": {
    "event-target-shim": "^2.0.0",
    "express": "^4.15.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "uuid": "^3.0.1",
    "webpack": "^2.6.1"
  },
